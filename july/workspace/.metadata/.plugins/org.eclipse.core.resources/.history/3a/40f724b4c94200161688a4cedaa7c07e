package Section1;

public class Z3_5
	{
		static int test1(int i)
			{
				return ++i;
			}

		static int test2(int i)
			{
				int k = test1(i++);
				int l = test2(++i);
				return test1 k+l;
			}

		public static void main(String[] args)
			{
				int i = 1;
				int j = test2(3);
				System.out.println("j=" + j);
			}
	}
