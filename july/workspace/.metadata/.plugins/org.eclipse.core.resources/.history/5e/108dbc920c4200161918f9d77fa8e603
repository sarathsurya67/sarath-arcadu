package Section1;

public class Y2
	{
		static int test1()
		{
			System.out.println("from test1");
			return 10;
		}
		static int test2()
		{
			System.out.println("from test2");
			return test1();
		}
		static int test3()
		{
			System.out.println("from test3");
			return test1()+test2();
		}
		public static void main(String[] args) 
		{
			System.out.println(test1());//fromtest1
													//10
			System.out.println(test2());//from test1..from test2 10
			System.out.println(test3());//from test1fromtest2 20 from test3
			//test2();
		}
	}
